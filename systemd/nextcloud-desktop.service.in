[Unit]
Description=Nextcloud desktop user service
Wants=network-online.target

[Service]
ExecStart=@bindir@/nextcloud --background
Restart=on-failure
RestartSec=5s
NoNewPrivileges=true
RestrictRealtime=true

[Install]
WantedBy=default.target
